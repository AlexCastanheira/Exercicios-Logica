import java.util.*;

public class Main {
   public static void main(String[] args) {
      Scanner in = new Scanner(System.in);

      int n1;

      n1 = in.nextInt();

      if (n1 % 2 == 0 || n1 % 3 == 0) {
         System.out.println("Não é primo");
      } else {
         System.out.println("É primo");
      }

   }
}
