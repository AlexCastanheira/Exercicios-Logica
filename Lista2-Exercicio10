import java.util.*;

public class Main {

   // INICIO

   public static void main(String[] args) {
      Scanner in = new Scanner(System.in);

      int idade;

      System.out.print("Informe sua idade: ");
      idade = in.nextInt();

      if (idade >= 18) {
         System.out.println("Voce est치 na categoria ADULTO.\n");
      } else if (idade >= 14) {
         System.out.println("Voce est치 na categoria JUVENIL.\n");
      } else if (idade >= 9) {
         System.out.println("Voce est치 na categoria INFANTIL.\n");
      } else {
         System.out.println("Voce est치 na categoria MIRIM.\n");
      }

   }
}
