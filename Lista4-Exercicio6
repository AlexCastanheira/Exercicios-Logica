import java.util.*;

public class Exec4e6 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        System.out.print("Digite um número: ");
        int n1 = in.nextInt();
         

        while (n1 < 1 || n1 > 10) {
            System.out.println(n1);
            System.out.print("Digite um número: ");
            n1 = in.nextInt();
        }

    }

}
