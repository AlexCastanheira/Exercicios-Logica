import java.util.Scanner;

public class Main {
   public static void main(String[] args) {
      Scanner in = new Scanner(System.in);

      int numero1, numero2;
      System.out.println("\nDigite um numero inteiro:\n");
      numero1 = in.nextInt();
      System.out.println("\nDigite outro numero inteiro:\n");
      numero2 = in.nextInt();

      if (numero2 == 0) {
         System.out.println("\nNão é possível dividir por zero\n");
      } else
         System.out.println(numero1 / numero2);

   }
}
