import java.util.Scanner;
 
 
public class Main {
   public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
      
    int numero1, numero2;
        System.out.println("\nDigite um numero inteiro:\n");
    numero1 = in.nextInt();
        System.out.println("\nDigite outro numero inteiro:\n");
    numero2 = in.nextInt();

        if (numero1 > numero2) { System.out.println("\nO numero " + numero1 + " é maior\n");}
        if (numero1 == numero2) { System.out.println("\nOs numeros são iguais\n");}
        if (numero1 < numero2) { System.out.println("\nO numero " + numero2 + " é maior\n");}
   
    
   }
}
